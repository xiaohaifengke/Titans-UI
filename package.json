{
  "name": "titans-ui",
  "version": "0.2.5",
  "description": "前端自研组件库",
  "author": "Jhail <Jhail@foxmail.com>",
  "homepage": "https://github.com/xiaohaifengke/Titans-UI#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "style": "dist/index.css",
  "unpkg": "dist/index.js",
  "jsdevlivr": "dist/index.js",
  "sideEffects": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaohaifengke/Titans-UI.git"
  },
  "bugs": {
    "url": "https://github.com/xiaohaifengke/Titans-UI/issues"
  },
  "files": [
    "dist",
    "es",
    "src"
  ],
  "keywords": [
    "titans-ui",
    "Titans-UI",
    "vue",
    "vue3",
    "rollup",
    "typescript"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:es && npm run build:umd",
    "build:pkg": "webpack -c ./build/webpack.js",
    "build:doc": "vue-cli-service build",
    "build:es": "rollup -c ./build/rollup.config.esm.js",
    "build:umd": "rollup -c ./build/rollup.config.umd.js && cross-env NODE_ENV=production rollup -c ./build/rollup.config.umd.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "prepare": "husky install",
    "cz": "git-cz",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "custom:publish": "publish-manager cleanse && npm publish && publish-manager restore"
  },
  "dependencies": {
    "vue": "^3.2.20"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/prompt-cli": "^13.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-plugin-vuex": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.2.14",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.1.0",
    "@vue/test-utils": "^2.0.0-rc.14",
    "autoprefixer": "^9.8.8",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.4",
    "commitlint": "^13.1.0",
    "conventional-changelog": "^3.1.24",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.18.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^1.6.2",
    "post-loader": "^2.0.0",
    "postcss-loader": "^4.3.0",
    "prettier": "^2.4.1",
    "publish-manager": "0.0.1",
    "rollup": "^2.57.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.42.1",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "ts-loader": "^8.3.0",
    "typescript": "^4.1.5",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.8.0",
    "webpack-merge": "^5.8.0"
  },
  "lint-staged": {
    "(src|story)/**/*.vue": [
      "stylelint --fix",
      "prettier --write",
      "eslint"
    ],
    "(src|story)/**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint"
    ],
    "(src|story)/**/*.((s(c|a)|c)ss)": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
